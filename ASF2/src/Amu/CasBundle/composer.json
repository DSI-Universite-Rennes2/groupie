{
  "name": "amu/cas-bundle",
  "type": "symfony-bundle",
  "version": "master",
  "description": "Implements CAS Authentication on your website",
  "keywords": ["amu", "cas", "authentication", "jasig"],
  "homepage": "http://dosi.univ-amu.fr/",
  "license": "GPL",
  "authors": [
    {
      "name": "UBEDA Michel",
      "email": "michel.ubeda@univ-amu.fr"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "jasig/phpcas": "~1.3",
    "symfony/symfony": ">=2.7"
  },
  "autoload": {
    "psr-0": {
      "Amu\\CasBundle": ""
    },
    "classmap": [
      "Amu/CasBundle/Service/Cas.php"
    ]
  },
  "target-dir": "Amu/CasBundle",
  "minimum-stability": "dev",
  "extra": {
      "branch-alias": {
        "dev-master": "1.0.1",
        "dev-v1.0.0": "1.0.0",
        "dev-v1.0.1": "1.0.1"
      }
    }

}
