<table {% if class is defined %}class='{{ class }}'{% endif %} >
    <thead>
        <tr>
            <th scope="col">Key</th>
            <th scope="col">Value</th>
        </tr>
    </thead>
    <tbody>
        {% for key in data|keys|sort %}
            <tr>
                <th>{{ key }}</th>
                {# JSON_UNESCAPED_SLASHES = 64, JSON_UNESCAPED_UNICODE = 256 #}
                  <td>{{ dump(data[key]) }}</td> {# |json_encode(64 b-or 256) #}
            </tr>
        {% endfor %}
    </tbody>
</table>
